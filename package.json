{
  "name": "microback-starter",
  "version": "0.0.2",
  "description": "Template for microservice using microback",
  "main": "index.js",
  "scripts": {
    "start": "microback",
    "test": "ava -v",
    "test:unit": "ava -v tests/unit/*.js",
    "test:e2e": "ava -v tests/e2e/*.js"
  },
  "author": "Sirajul",
  "license": "ISC",
  "ava": {
    "files": [
      "tests/e2e/*.js",
      "tests/unit/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "microback": "latest",
    "sharp": "^0.18.4",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "supertest": "^3.0.0"
  }
}
